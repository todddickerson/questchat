{
  "name": "questchat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "vercel-build": "npx prisma generate && npx next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate || true",
    "whop-proxy": "pnpm dlx @whop-apps/dev-proxy --command 'next dev --turbo'",
    "whop-dev": "pnpm dlx @whop-apps/dev-proxy --proxyPort 3000 --upstreamPort 3001 --command 'next dev --port 3001 --turbo'",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.38.5",
    "@whop-apps/sdk": "0.0.1-canary.117",
    "@whop/api": "latest",
    "@whop/react": "latest",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "next": "14.0.4",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "playwright": "^1.55.0",
    "typescript": "^5",
    "vercel": "^47.0.5"
  }
}
